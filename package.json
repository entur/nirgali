{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@entur/alert": "0.14.3",
    "@entur/auth-provider": "1.1.1",
    "@entur/button": "2.10.15",
    "@entur/datepicker": "3.0.10",
    "@entur/form": "5.4.20",
    "@entur/icons": "5.8.1",
    "@entur/layout": "2.1.53",
    "@entur/tab": "0.4.57",
    "@entur/tokens": "3.12.0",
    "@entur/tooltip": "2.6.41",
    "@entur/typography": "1.8.20",
    "apollo-fetch": "0.7.0",
    "bootstrap": "5.3.2",
    "date-fns": "3.0.6",
    "firebase": "10.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.1",
    "react-select": "3.2.0",
    "react-super-responsive-table": "5.2.2",
    "react-windowed-select": "2.0.5"
  },
  "scripts": {
    "copy:config": "cp config/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:firebase start:react",
    "start:firebase": "firebase serve -p 4000",
    "start:react": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:functions": "jest functions",
    "format": "prettier --write \"{src,functions,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,functions,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,functions,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "18.19.4",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "react-scripts": "5.0.1",
    "sass": "1.62.1",
    "typescript": "4.9.5"
  },
  "proxy": "http://localhost:4000"
}
