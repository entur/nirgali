{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "4.0.9",
    "@entur/alert": "0.17.18",
    "@entur/button": "3.3.18",
    "@entur/datepicker": "11.4.3",
    "@entur/dropdown": "8.0.0",
    "@entur/form": "9.0.1",
    "@entur/icons": "8.2.0",
    "@entur/layout": "3.2.2",
    "@entur/tab": "0.5.7",
    "@entur/table": "4.9.20",
    "@entur/tokens": "3.20.0",
    "@entur/tooltip": "5.2.17",
    "@entur/typography": "2.0.3",
    "@internationalized/date": "3.10.0",
    "bootstrap": "5.3.8",
    "graphql": "16.12.0",
    "oidc-client-ts": "3.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-oidc-context": "3.3.0",
    "react-router-dom": "7.9.6",
    "react-select": "5.10.2",
    "react-super-responsive-table": "6.0.2",
    "react-windowed-select": "5.2.0",
    "rxjs": "7.8.2"
  },
  "scripts": {
    "copy:config": "cp .github/environments/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:react",
    "start:react": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --watch=false --coverage --coverage.reporter=lcov --reporter=vitest-sonar-reporter --outputFile.vitest-sonar-reporter=test-report.xml --reporter=default",
    "format": "prettier --write \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.2",
    "@vitest/coverage-v8": "3.2.4",
    "husky": "9.1.7",
    "jsdom": "27.2.0",
    "lint-staged": "16.2.6",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "sass": "1.94.0",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "vitest-sonar-reporter": "3.0.0"
  }
}
