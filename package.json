{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@entur/alert": "0.15.16",
    "@entur/auth-provider": "1.1.1",
    "@entur/button": "3.2.7",
    "@entur/datepicker": "9.1.3",
    "@entur/form": "7.0.34",
    "@entur/icons": "6.13.2",
    "@entur/layout": "2.1.54",
    "@entur/tab": "0.4.58",
    "@entur/tokens": "3.12.0",
    "@entur/tooltip": "2.6.43",
    "@entur/typography": "1.8.21",
    "@internationalized/date": "3.5.1",
    "apollo-fetch": "0.7.0",
    "bootstrap": "5.3.2",
    "firebase": "10.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.3",
    "react-select": "5.8.0",
    "react-super-responsive-table": "5.2.2",
    "react-windowed-select": "5.1.1"
  },
  "scripts": {
    "copy:config": "cp config/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:firebase start:react",
    "start:firebase": "firebase serve -p 4000",
    "start:react": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:functions": "jest functions",
    "format": "prettier --write \"{src,functions,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,functions,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,functions,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/jest": "29.5.11",
    "@types/node": "18.19.9",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/typescript-estree": "6.19.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.4",
    "react-scripts": "5.0.1",
    "sass": "1.70.0",
    "typescript": "5.3.3"
  },
  "proxy": "http://localhost:4000"
}
