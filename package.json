{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@entur/alert": "0.15.18",
    "@entur/auth-provider": "1.1.1",
    "@entur/button": "3.2.9",
    "@entur/datepicker": "9.1.5",
    "@entur/dropdown": "5.1.3",
    "@entur/form": "7.0.36",
    "@entur/icons": "6.15.0",
    "@entur/layout": "2.1.56",
    "@entur/tab": "0.4.58",
    "@entur/table": "4.6.38",
    "@entur/tokens": "3.12.0",
    "@entur/tooltip": "2.6.45",
    "@entur/typography": "1.8.23",
    "@internationalized/date": "3.5.5",
    "apollo-fetch": "0.7.0",
    "bootstrap": "5.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.26.2",
    "react-select": "5.8.1",
    "react-super-responsive-table": "6.0.0",
    "react-windowed-select": "5.2.0"
  },
  "scripts": {
    "copy:config": "cp .github/environments/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:react",
    "start:react": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --watch=false --coverage --coverage.reporter=lcov --reporter=vitest-sonar-reporter --outputFile.vitest-sonar-reporter=test-report.xml --reporter=default",
    "format": "prettier --write \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "18.19.50",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "2.1.1",
    "husky": "8.0.3",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "npm-run-all2": "5.0.0",
    "prettier": "3.3.3",
    "sass": "1.79.3",
    "typescript": "5.6.2",
    "vitest": "2.1.1",
    "vitest-sonar-reporter": "2.0.0",
    "vite": "5.4.7",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "5.0.1"
  }
}
