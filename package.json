{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.11.10",
    "@entur/alert": "0.16.21",
    "@entur/button": "3.2.36",
    "@entur/datepicker": "10.0.10",
    "@entur/dropdown": "6.0.10",
    "@entur/form": "8.1.7",
    "@entur/icons": "7.5.0",
    "@entur/layout": "2.3.20",
    "@entur/tab": "0.4.76",
    "@entur/table": "4.8.24",
    "@entur/tokens": "3.17.3",
    "@entur/tooltip": "5.1.3",
    "@entur/typography": "1.8.49",
    "@internationalized/date": "3.6.0",
    "bootstrap": "5.3.3",
    "graphql": "16.9.0",
    "oidc-client-ts": "3.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-oidc-context": "3.2.0",
    "react-router-dom": "6.28.0",
    "react-select": "5.8.3",
    "react-super-responsive-table": "6.0.0",
    "react-windowed-select": "5.2.0"
  },
  "scripts": {
    "copy:config": "cp .github/environments/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:react",
    "start:react": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --watch=false --coverage --coverage.reporter=lcov --reporter=vitest-sonar-reporter --outputFile.vitest-sonar-reporter=test-report.xml --reporter=default",
    "format": "prettier --write \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "18.19.66",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "3.7.1",
    "@vitest/coverage-v8": "2.1.5",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "npm-run-all2": "6.2.6",
    "prettier": "3.3.3",
    "sass": "1.81.0",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "2.1.5",
    "vitest-sonar-reporter": "2.0.0"
  }
}
