{
  "name": "nirgali",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.13.9",
    "@entur/alert": "0.17.12",
    "@entur/button": "3.3.12",
    "@entur/datepicker": "11.2.0",
    "@entur/dropdown": "7.3.1",
    "@entur/form": "8.3.1",
    "@entur/icons": "7.14.0",
    "@entur/layout": "3.1.8",
    "@entur/tab": "0.5.4",
    "@entur/table": "4.9.13",
    "@entur/tokens": "3.19.1",
    "@entur/tooltip": "5.2.12",
    "@entur/typography": "1.9.12",
    "@internationalized/date": "3.8.2",
    "bootstrap": "5.3.7",
    "graphql": "16.11.0",
    "oidc-client-ts": "3.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-oidc-context": "3.3.0",
    "react-router-dom": "6.30.1",
    "react-select": "5.10.2",
    "react-super-responsive-table": "6.0.2",
    "react-windowed-select": "5.2.0"
  },
  "scripts": {
    "copy:config": "cp .github/environments/config-dev.json public/config.json",
    "prestart": "run-p --race copy:config",
    "start": "run-p --race start:react",
    "start:react": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --watch=false --coverage --coverage.reporter=lcov --reporter=vitest-sonar-reporter --outputFile.vitest-sonar-reporter=test-report.xml --reporter=default",
    "format": "prettier --write \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"{src,config}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,config}/**!(*node_modules)/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "18.19.122",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react-swc": "4.0.0",
    "@vitest/coverage-v8": "2.1.9",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.5.2",
    "npm-run-all2": "6.2.6",
    "prettier": "3.6.2",
    "sass": "1.90.0",
    "typescript": "5.9.2",
    "vite": "7.1.1",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "vitest-sonar-reporter": "2.0.4"
  }
}
